{
  "name": "mlathrom/craft-remix",
  "description": "Transform and output your entry titles and slugs with this custom field. Great for sorting, filtering, SEO, and more.",
  "type": "craft-plugin",
  "license": "proprietary",
  "support": {
    "email": "matt@mlathrom.com"
  },
  "require": {
    "php": ">=8.0.2",
    "craftcms/cms": "^4.0.0"
  },
  "require-dev": {
    "craftcms/ecs": "dev-main",
    "craftcms/phpstan": "dev-main"
  },
  "autoload": {
    "psr-4": {
      "mlathrom\\craftremix\\": "src/"
    }
  },
  "extra": {
    "handle": "remix",
    "name": "Remix",
    "developer": "Matt Lathrom",
    "documentationUrl": ""
  },
  "scripts": {
    "check-cs": "ecs check --ansi",
    "fix-cs": "ecs check --ansi --fix",
    "phpstan": "phpstan --memory-limit=1G"
  },
  "config": {
    "sort-packages": true,
    "platform": {
      "php": "8.0.2"
    },
    "allow-plugins": {
      "yiisoft/yii2-composer": true,
      "craftcms/plugin-installer": true
    }
  }
}